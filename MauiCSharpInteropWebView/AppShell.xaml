<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="MauiCSharpInteropWebView.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:MauiCSharpInteropWebView"
    Shell.FlyoutBehavior="Flyout">

    <FlyoutItem FlyoutDisplayOptions="AsMultipleItems">
        <ShellContent Title="Home" ContentTemplate="{DataTemplate local:MainPage}" Route="MainPage" />        
        <ShellContent Title="Embedded Resource Sample" ContentTemplate="{DataTemplate local:EmbeddedResourceSample}" Route="EmbeddedResourceSample"/>
        <ShellContent Title="Load From File System Sample" ContentTemplate="{DataTemplate local:LoadFromFileSystemSample}" Route="LoadFromFileSystemSample"/>
    </FlyoutItem>
</Shell>
